{
  "name": "frenchfrysaysgeodeplugin",
  "displayName": "frenchfrysays's Geode Plugin",
  "description": "",
  "publisher": "frenchfrysays",
  "author": "frenchfrysays",
  "version": "1.2.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "geodeplugin.builddefault",
        "title": "Geode: Build for Default Platform"
      },
      {
        "command": "geodeplugin.buildwin",
        "title": "Geode: Build for Windows"
      },
      {
        "command": "geodeplugin.buildmac",
        "title": "Geode: Build for macOS"
      },
      {
        "command": "geodeplugin.buildios",
        "title": "Geode: Build for iOS"
      },
      {
        "command": "geodeplugin.buildandroid",
        "title": "Geode: Build for Android"
      },
      {
        "command": "geodeplugin.activateextension",
        "title": "Geode: Activate Extension"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "build": "vsce package --allow-missing-repository"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "24.x",
    "@types/vscode": "^1.104.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.34.0",
    "typescript": "^5.9.2",
    "@vscode/vsce": "^3.6.0"
  }
}
